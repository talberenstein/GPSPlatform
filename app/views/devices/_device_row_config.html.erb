<tr data-id="<%= device.id %>">
  <td class="">
    <%= device.name %>
  </td>
  <td class="">
    <%= device.imei %>
  </td>
  <td class="">
    <%= device.phone %>
  </td>
  <td class="">
    <%= device.driver.to_s %>
  </td>
  <td class="">
    <%= device.company.name %>
  </td>
  <td class="">
    <%= device.group.try(:name) %>
  </td>
  <td class="">
    <a href="#edit_device_modal" class="edit_device_btn"><%= image_tag 'edit.svg' %></a>
  </td>
  <td class="">
    <%= link_to device, method: :delete, remote: true, data: { confirm: 'Esta seguro?' }, class: '' do %>
    <%= image_tag 'delete.svg' %>
    <% end %>
  </td>
  <td class="">
  </td>
</tr>
