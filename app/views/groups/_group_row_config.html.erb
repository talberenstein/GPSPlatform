    <tr data-id="<%= group.id %>">
        <td class="">
            <%= group.name %>
        </td>
        <td class="">
            <%= group.company.name %>
        </td>
        <td class="">
            <a href="#edit_group_modal" class="edit_group_btn"><%= image_tag 'edit.svg' %></a>
        </td>
        <td class="">
            <%= link_to group, method: :delete, remote: true, data: { confirm: 'Esta seguro?' }, class: '' do %>
                <%= image_tag 'delete.svg' %>
            <% end %>
        </td>
        <td class="">
        </td>
    </tr>
