    <tr data-id="<%= geo_zone.id %>">
        <td class="">
            <%= geo_zone.name %>
        </td>
        <td class="">
            <%= geo_zone.enter_alert? ? 'Si' : 'No' %>
        </td>
        <td class="">
            <%= geo_zone.exit_alert? ? 'Si' : 'No' %>
        </td>
        <td class="">
            <%= geo_zone.company %>
        </td>
        <td class="">
            <a href="#edit_geo_zone_modal" class="edit_geo_zone_btn"><%= image_tag 'edit.svg' %></a>
        </td>
        <td class="">
            <%= link_to geo_zone, method: :delete, remote: true, data: { confirm: 'Esta seguro?' }, class: '' do %>
                <%= image_tag 'delete.svg' %>
            <% end %>
        </td>
        <td class="">
        </td>
    </tr>
